{
  "name": "angular2-base-library",
  "version": "1.0.0",
  "description": "a seed project for Angular2 libraries",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server -c bs-config.json\"",
    "code": "tsc && concurrently \"tsc -w\" \"lite-server -c bs-config.json\" \"karma start karma.conf.js\"",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server -c bs-config.json",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "keywords": [
    "Angular2"
  ],
  "author": "JayChase",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.0-rc.2",
    "@angular/compiler": "2.0.0-rc.2",
    "@angular/core": "2.0.0-rc.2",
    "@angular/http": "2.0.0-rc.2",
    "@angular/platform-browser": "2.0.0-rc.2",
    "@angular/platform-browser-dynamic": "2.0.0-rc.2",
    "@angular/router": "2.0.0-rc.2",
    "core-js": "^2.4.0",
    "express": "^4.13.4",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "canonical-path": "0.0.2",
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "jasmine-core": "2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-htmlfile-reporter": "^0.2.2",
    "karma-jasmine": "^0.3.8",
    "karma-verbose-reporter": "0.0.3",
    "lodash": "^4.11.1",
    "protractor": "^3.3.0",
    "tslint": "^3.7.4",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  },
  "repository": {}
}
